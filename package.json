{
	"name": "claude-cmd",
	"version": "0.1.0",
	"description": "CLI package manager for Claude Code slash commands",
	"type": "module",
	"main": "dist/main.js",
	"bin": {
		"claude-cmd": "dist/main.js"
	},
	"scripts": {
		"test": "bun test",
		"build": "bun build src/main.ts --outdir=dist --target=bun",
		"start": "bun src/main.ts",
		"dev": "bun --watch src/main.ts",
		"typecheck": "tsc --noEmit",
		"check": "bun run biome check",
		"check-fix": "bun run biome check --write --unsafe"
	},
	"keywords": [
		"claude",
		"cli",
		"commands",
		"package-manager"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@logtape/logtape": "^1.0.0",
		"commander": "^14.0.0",
		"gray-matter": "^4.0.3",
		"proper-lockfile": "^4.1.2",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@types/bun": "latest",
		"typescript": "^5"
	}
}
